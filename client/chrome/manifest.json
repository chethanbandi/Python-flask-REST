{
  "manifest_version": 2,
  
  "name": "Pointers.io history grabber",
  "description": "This extension grabes all the history and uploads to a server",
  "version": "1.0",

  "permissions": [
    "history",
    "http://apps.sairahul.com/history",
    "http://localhost:5000/history"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "history.html"
  }
}
